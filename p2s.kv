<FilesScreen>:
    name: 'filescreen'
    BoxLayout:
        orientation: 'vertical'
        Button:
            font_size: 20
            size_hint: None, None
            size: self.texture_size[0]+10, self.texture_size[1]+5
            text: "back"
            on_press: root.manager.current = 'appscreen'
        Files:
            multiselect: True

<AppScreen>:
    name: 'appscreen'
    Button:
        text: 'ALGO ACA'
        on_press: root.manager.current = 'filescreen'

<ScreenMan>:
    FilesScreen
    AppScreen